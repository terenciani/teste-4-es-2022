        "def numeros[], retorno, pares[], impares[], i" -> "use i"
        "use i" -> "use numeros[]" [label="v"]
        "use numeros[]" -> "def pares[]; use numeros[]" [label="v"]
        "use numeros[]" -> "def impares[]; use numeros[]" [label="f"]
